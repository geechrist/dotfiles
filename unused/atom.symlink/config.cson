"*":
  editor:
    invisibles: {}
    fontSize: 15
    showIndentGuide: true
    showInvisibles: true
    scrollPastEnd: true
    fontFamily: "Hack"
  core:
    hideGitIgnoredFiles: true
    autoHideMenuBar: true
    projectHome: "~/Code/"
    disabledPackages: [
      "metrics"
      "exception-reporting"
    ]
  welcome:
    showOnStartup: false
  linter: {}
  "go-plus":
    goPath: "~/Code/Go"
  "go-rename": {}
  "tree-view":
    hideVcsIgnoredFiles: true
